{
  "extends": "./tsconfig.json",
  "compileOnSave": true,
  "compilerOptions": {
    "sourceMap": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "paths": {
      "@/model/*": ["./src/model/*"],
      "@/database/*": ["./src/model/database/*"],
      "@/stravaApi": ["./src/model/stravaApi/index"],
      "@/stravaApi/*": ["./src/model/stravaApi/*"],
      "@/ffvlApi": ["./src/model/ffvlApi/index"],
      "@/ffvlApi/*": ["./src/model/ffvlApi/*"],
      "@/tasks/model": ["./src/tasks/model"],
      "@/tasks/*": ["./src/tasks/*"],
      "@/api/*": ["./src/api/*"],
      "@/webhooks/*": ["./src/webhooks/*"],
      "@/utils/*": ["./src/utils/*"],
      "@/jwt": ["./src/jwt"]
    }
  },
  "exclude": [
    "src/**/*.test.ts",
    "src/**/*.disabled.ts",
    "node_modules/*"
  ]
}