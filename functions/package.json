{
  "author": "theostanton",
  "name": "functions",
  "version": "0.1.0",
  "main": "./index.js",
  "config": {
    "dirBuild": "dist",
    "dirSrc": "src"
  },
  "scripts": {
    "test": "vitest run",
    "buildDev": "tsc && tsc-alias",
    "buildProd": "tsc && tsconfig-replace-paths --project tsconfig.json",
    "devApi": "tsc -p dev.tsconfig.json && node --watch -r ts-node/register -r tsconfig-paths/register src/api/dev.ts",
    "devTasks": "tsc -p dev.tsconfig.json && node --watch -r ts-node/register -r tsconfig-paths/register src/tasks/dev.ts",
    "devWebhooks": "tsc -p dev.tsconfig.json && nodemon --inspect src/webhooks/dev.ts"
  },
  "dependencies": {
    "@google-cloud/tasks": "^6.0.1",
    "axios": "^1.8.4",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "ts-postgres": "^2.0.4"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^10.24.2",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/cookie-parser": "^1.4.8",
    "testcontainers": "^10.24.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-replace-paths": "^0.0.14"
  }
}
